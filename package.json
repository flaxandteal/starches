  },
  "name": "starches-coral",
  "version": "0.0.1",
  "description": "Static Arches export for Coral",
  "main": "reindex.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "dotenv vite",
    "build": "dotenv -- tsc && vite build",
    "preview": "dotenv vite preview",
    "reindex": "dotenv -- bash -c 'rm -f prebuild/preindex/* docs/definitions/business_data/* && for data in prebuild/business_data/*; do echo DATA $data; dotenv -- node --loader ts-node/esm/transpile-only utils/preindex.ts $data; done && dotenv -- node --loader ts-node/esm/transpile-only utils/reindex.ts'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "mkdir pagefind; cd pagefind; git init; git remote add -f origin https://github.com/flaxandteal/pagefind.git; git config core.sparseCheckout true; echo \"wrappers/node\" >> .git/info/sparse-checkout; git pull --depth=1 origin feat/get-index-catalogue--1.3.0; cd ..; npm i ./pagefind/wrappers/node/"
  },
  "author": "Phil Weir",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^22.14.0",
    "dotenv-cli": "^8.0.0",
    "potree-core": "^2.0.10",
    "three": "^0.175.0",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "typescript": "^5.8.3",
    "vite": "^6.2.5",
    "vite-hugo-plugin": "^5.1.0"
  }
}
